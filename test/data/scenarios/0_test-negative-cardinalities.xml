        <maxCardinality>-1</maxCardinality>
    </resource>
</resources>
<requirements>
    <requirement>
        <spatial-requirement>
            <!-- where it is required -->
            <location>
                <id>l1</id>
            </location>
        </spatial-requirement>
        <temporal-requirement>
            <from>t0</from>
            <to>t1</to>
        </temporal-requirement>

        <!-- what is required at this very position -->
